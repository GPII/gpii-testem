{
    "sources": {
        "md": [ "./*.md", "./docs/*.md" ],
        "js": [ "./src/**/*.js", "./tests/**/*.js", "./*.js", "!./tests/testem-fixtures/instrumented/**/*.js" ],
        "json": [ "src/**/*.json", "tests/**/*.json", "./*.json" ],
